XF86AudioLowerVolume
    pulsemixer --change-volume -5
  
XF86AudioRaiseVolume
    pulsemixer --change-volume +5

XF86AudioMute
    pulsemixer --toggle-mute

XF86MonBrightnessDown
    brightnessctl set 15-

XF86MonBrightnessUp
    brightnessctl set 15+

# alt + minus
#     amixer sset PCM 5%-
# 
# alt + equal
#     amixer sset PCM 5%+
# 
# alt + 0
#     amixer sset PCM toggle

alt + p
    playerctl -p spotify play-pause

alt + bracketleft 
    playerctl -p spotify previous

alt + bracketright
    playerctl -p spotify next

Print
    maim "/home/david/Pictures/Screenshots/$(date +%Y-%m-%d_%H%M).png"

shift + Print
    maim -s "/home/david/Pictures/Screenshots/$(date +%Y-%m-%d_%H%M).png"

